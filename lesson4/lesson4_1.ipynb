    "import math\n",
    "\n",
    "age = 17\n",
    "age >= 18\n",
    "\n",
    "\n",
    "if (age >= 18):\n",
    "    print(\"可以考駕照\")\n",
    "else:\n",
    "    print(\"不可以考駕照\")\n",
    "\n",
    "nScore = int(input(\"請輸入分數(最高300分):\"))\n",
    "strIsAdd = input(\"學生是否符合加分條件?(y,n)\")\n",
    "\n",
    "if (strIsAdd == \"y\"):\n",
    "    nScore *= 1.05\n",
    "    nScore = math.ceil(nScore)\n",
    "\n",
    "print(f\"學生分數是: {nScore}\")\n"
